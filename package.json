{
  "name": "capacitor-background-geolocation",
  "version": "0.1.6",
  "description": "A native plugin to get geolocation updates also when app is in background",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "seididieci",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@capacitor/ios": "^2.4.7",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "lodash": ">=4.17.21",
    "prettier": "^2.3.0",
    "prettier-plugin-java": "^1.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "swiftlint": "^1.0.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.7"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorBackgroundGeolocation.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "geolocation",
    "background"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/seididieci/capacitor-backround-geolocation.git"
  },
  "bugs": {
    "url": "https://github.com/seididieci/capacitor-backround-geolocation.git/issues"
  }
}
